package com.cts.training.day6;

public class Tread2 implements Runnable {
	 
	 @Override
		public void run() {
		 for(int i=1; i<10;i++)
			 
		 {
			
	           int td=4;
	           td = td*i;
				
			 System.out.println("Thread-2:"+td);
			 }
			 try
			 {
				 Thread.sleep(2000);
			 }
			 catch (InterruptedException e)
			 {
				  e.printStackTrace();
			 }
		 }
		}