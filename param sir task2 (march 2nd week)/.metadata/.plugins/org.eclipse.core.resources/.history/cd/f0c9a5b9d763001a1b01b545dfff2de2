	<sql:query var="rs" sql="select * from employee">

	</sql:query>
	<c:forEach var="row" items="${rs.rows}">
		<c:out value="${row.id }" /> 
		<c:out value="${row.name }" />
		<c:out value="${row.address }" />
		<c:out value="${row.designation}" />
		<c:out value="${row.age }" />
		<c:out value="${row.salary }" />
		<c:out value="${row.phone }" /><br/>
